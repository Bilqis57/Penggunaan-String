#include <iostream>
using namespace std;

int main(){
    float  NilaiS1,NilaiS2,NilaiS3,NilaiS4,keseluruhan,rata_rata;
    string ratarata1,ratarata2,ratarata3;
    ratarata1="Baik";
    ratarata2="Cukup";
    ratarata3="Kurang";
    NilaiS1=90;
    NilaiS2=98;
    NilaiS3=95;
    NilaiS4=100;
    keseluruhan=NilaiS1+NilaiS2+NilaiS3+NilaiS4;
    rata_rata= keseluruhan/4;
    cout<<"Hasil Nilai Rata Rata Nya Adalah = "<< rata_rata<<endl;
    if (rata_rata>=80){
        cout<<"Nilai Anda: "<< ratarata1 <<endl;
    }
    else if (rata_rata<=79 && rata_rata>=60){
        cout<<"Nilai Anda: "<< ratarata2 << endl;
    }
    else{
        cout<<"Nilai Anda: "    << ratarata3 <<endl;
    }
}
    